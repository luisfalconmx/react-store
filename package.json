{
  "name": "@luisfalconmx/spa-store",
  "version": "0.0.1",
  "description": "Single Page Application with React, React Router, PostCSS, Webpack, Docker and Docker Compose.",
  "main": "src/index.js",
  "scripts": {
    "start": "yarn run dev",
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "analyze": "webpack --mode development --env analyze",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "api": "json-server --watch api/data.json --port 4000"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{js,jsx,sss,md}": "prettier --write",
    "*.{css,pcss,sss}": "stylelint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisfalconmx/spa-store.git"
  },
  "keywords": [
    "react",
    "babel",
    "spa",
    "webpack",
    "postcss",
    "react-router",
    "docker-compose",
    "single-page-app",
    "ecmascript6",
    "react-context",
    "react-hooks"
  ],
  "author": "luisfalconmx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luisfalconmx/spa-store/issues"
  },
  "homepage": "https://github.com/luisfalconmx/spa-store#readme",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@squoosh/lib": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "critters-webpack-plugin": "^3.0.2",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.16",
    "dotenv": "^14.3.2",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": ">=6",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "json-server": "^0.17.0",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "^2.5.3",
    "modern-normalize": "^1.1.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.67.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "axios": "^0.25.0",
