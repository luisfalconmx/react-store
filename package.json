{
  "name": "@luisfalconmx/react-store",
  "version": "1.0.0",
  "description": "Single Page Application created with React, React Router, PostCSS and Webpack",
  "main": "src/index.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "yarn run dev",
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "analyze": "webpack --mode development --env analyze",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "api": "json-server --watch api/response.json --port 4000",
    "lint": "eslint --fix .",
    "stylelint": "stylelint --fix '**/*.{css,pcss,sss}'",
    "pretty": "prettier --ignore-unknown --write ."
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*": "prettier --ignore-unknown --write",
    "*.{css,pcss,sss}": "stylelint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisfalconmx/react-store.git"
  },
  "keywords": [
    "react",
    "babel",
    "spa",
    "webpack",
    "postcss",
    "react-router",
    "single-page-app",
    "ecmascript6",
    "react-context",
    "react-hooks"
  ],
  "author": "luisfalconmx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luisfalconmx/react-store/issues"
  },
  "homepage": "https://github.com/luisfalconmx/react-store#readme",
  "dependencies": {
    "@splidejs/react-splide": "^0.6.20",
    "axios": "^1.6.0",
    "classnames": "^2.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-router-dom": "^6.2.1",
    "react-svg": "^14.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@squoosh/lib": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "clean-webpack-plugin": "^4.0.0",
    "critters-webpack-plugin": "^3.0.2",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.16",
    "dotenv": "^14.3.2",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.5.3",
    "modern-normalize": "^1.1.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpackbar": "^5.0.2"
  }
}
